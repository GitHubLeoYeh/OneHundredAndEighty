                                <Slider x:Name="Cam4RoiPosYSlider"
                                        Grid.Row="1"
                                        Grid.RowSpan="7"
                                        Grid.Column="11"
                                        Style="{StaticResource MainWindowSetupSliderStyle}"
                                        Value="{Binding Cam4RoiPosYSliderValue, Mode=TwoWay}"
                                        IsEnabled="{Binding IsCamsSetupRunning}" />
                                <Slider x:Name="Cam4RoiHeightSlider"
                                        Grid.Row="1"
                                        Grid.RowSpan="7"
                                        Grid.Column="13"
                                        Style="{StaticResource MainWindowSetupSliderStyle}"
                                        Value="{Binding Cam4RoiHeightSliderValue, Mode=TwoWay}"
                                        IsEnabled="{Binding IsCamsSetupRunning}" />
                                <Slider x:Name="Cam4SurfaceSlider"
                                        Grid.Row="1"
                                        Grid.RowSpan="7"
                                        Grid.Column="7"
                                        Style="{StaticResource MainWindowSetupSliderStyle}"
                                        Value="{Binding Cam4SurfaceSliderValue, Mode=TwoWay}"
                                        IsEnabled="{Binding IsCamsSetupRunning}" />
                                <Slider x:Name="Cam4SurfaceCenterSlider"
                                        Grid.Row="1"
                                        Grid.RowSpan="7"
                                        Grid.Column="9"
                                        Style="{StaticResource MainWindowSetupSliderStyle}"
                                        Value="{Binding Cam4SurfaceCenterSliderValue, Mode=TwoWay}"
                                        IsEnabled="{Binding IsCamsSetupRunning}" />
                                <Image x:Name="Cam4ImageBox"
                                       Grid.Row="1"
                                       Grid.RowSpan="5"
                                       Grid.Column="1"
                                       Source="{Binding CamImage, IsAsync=True}" />
                                <Image x:Name="Cam4ImageBoxRoi"
                                       Grid.Row="7"
                                       Grid.Column="1"
                                       Source="{Binding CamRoiImage, IsAsync=True}" />
                                <Label Content="Threshold"
                                       Grid.Row="9"
                                       Grid.Column="5"
                                       Style="{StaticResource MainWindowLabelStyle}"
                                       ToolTip="ROI image threshold slider. Stick dart into dartboard and set slider in position, &#xA;when you can see only white dart contour, without any light noise from sides of image" />
                                <Label Content="ROI Y"
                                       Grid.Row="9"
                                       Grid.Column="11"
                                       Style="{StaticResource MainWindowLabelStyle}"
                                       ToolTip="ROI Y position" />
                                <Label Content="ROI height"
                                       Grid.Row="9"
                                       Grid.Column="13"
                                       Style="{StaticResource MainWindowLabelStyle}"
                                       ToolTip="ROI height" />
                                <Label Content="Surface Y"
                                       Grid.Row="9"
                                       Grid.Column="7"
                                       Style="{StaticResource MainWindowLabelStyle}"
                                       ToolTip="Surface line of dartboard surface" />
                                <Label Content="Surface center"
                                       Grid.Row="9"
                                       Grid.Column="9"
                                       Style="{StaticResource MainWindowLabelStyle}"
                                       ToolTip="Surface center point. Stick dart into bull and set" />
                                <Button x:Name="Cam4StartButton"
                                        Content="Start"
                                        Grid.Row="1"
                                        Grid.Column="3"
                                        Click="CamSetupStartButtonClick"
                                        IsEnabled="{Binding IsCamsSetupRunning, Converter={StaticResource Inverted}}"
                                        Style="{StaticResource MainWindowButtonStyle}" />
                                <Button x:Name="Cam4StopButton"
                                        Content="Stop"
                                        Grid.Row="3"
                                        Grid.Column="3"
                                        Click="CamSetupStopButtonClick"
                                        IsEnabled="{Binding IsCamsSetupRunning}"
                                        Style="{StaticResource MainWindowButtonStyle}" />
                                <TextBox Grid.Row="5"
                                         Grid.Column="3"
                                         Grid.RowSpan="3"
                                         IsReadOnly="True"
                                         Text="{Binding ContoursBoxText, Mode=OneWay}"
                                         Style="{StaticResource MainWindowTextBoxStyle}" />
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
